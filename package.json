{
  "name": "crossing-tasks",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov <connormiha1@gmail.com>",
  "engines": {
    "node": ">=6.6.0",
    "npm": ">=3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/crossing-tasks.git"
  },
  "scripts": {
    "precommit": "NODE_ENV=pre_commit npm run lint",
    "prepush": "npm test",
    "build": "rm -fr build && NODE_ENV=production webpack",
    "lint": "gulp lint",
    "start": "webpack-dev-server",
    "coverage": "jest --coverage",
    "test": "jest --no-cache"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "styl",
      "css"
    ],
    "transform": {
      ".*": "<rootDir>/__tests__/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.spec\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "watchman": false,
    "notify": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.ts"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "2.7.5",
    "@types/history": "3.2.0",
    "@types/jest": "19.2.1",
    "@types/lodash": "4.14.55",
    "@types/react": "15.0.16",
    "@types/react-addons-test-utils": "0.14.17",
    "@types/react-dom": "0.14.23",
    "@types/react-redux": "4.4.37",
    "@types/react-router": "3.0.7",
    "@types/redux-thunk": "2.1.32",
    "@types/seamless-immutable": "6.1.2",
    "autoprefixer": "6.7.6",
    "babel-core": "6.23.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.23.0",
    "css-loader": "0.27.1",
    "csso": "2.3.2",
    "csso-loader": "0.2.2",
    "enzyme": "2.7.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "gulp": "3.9.1",
    "gulp-stylint": "4.0.0",
    "gulp-tslint": "7.1.0",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.2",
    "jest-cli": "19.0.2",
    "lodash": "4.17.4",
    "postcss": "5.2.16",
    "postcss-loader": "1.3.3",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-test-renderer": "15.4.2",
    "redux": "3.6.0",
    "redux-batched-actions": "0.1.5",
    "redux-devtools-extension": "2.13.0",
    "redux-thunk": "2.2.0",
    "seamless-immutable": "7.0.1",
    "style-loader": "0.13.2",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "ts-loader": "2.0.1",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "tslint-react": "2.5.0",
    "typed-css-modules": "0.2.0",
    "typed-css-modules-loader": "0.0.4",
    "typescript": "2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-sources": "0.2.0"
  }
}
