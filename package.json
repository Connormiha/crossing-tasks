{
  "name": "crossing-tasks",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov <connormiha1@gmail.com>",
  "engines": {
    "node": ">=6.6.0",
    "npm": ">=3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/crossing-tasks.git"
  },
  "scripts": {
    "precommit": "NODE_ENV=pre_commit npm run lint",
    "prepush": "npm test",
    "build": "rm -fr build && NODE_ENV=production webpack",
    "lint": "gulp lint",
    "start": "webpack-dev-server",
    "coverage": "jest --coverage",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "styl",
      "css"
    ],
    "transform": {
      ".*": "<rootDir>/__tests__/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.spec\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "notify": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.ts"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "2.7.1",
    "@types/jest": "16.0.3",
    "@types/lodash": "4.14.42",
    "@types/react": "0.14.55",
    "@types/react-addons-test-utils": "0.14.16",
    "@types/react-dom": "0.14.20",
    "@types/react-redux": "4.4.35",
    "@types/react-router": "2.0.41",
    "@types/redux-thunk": "2.1.32",
    "@types/seamless-immutable": "6.1.2",
    "autoprefixer": "6.6.1",
    "css-loader": "0.26.1",
    "csso": "2.3.1",
    "csso-loader": "0.2.1",
    "enzyme": "2.7.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-stylint": "4.0.0",
    "gulp-tslint": "7.0.1",
    "html-webpack-plugin": "2.26.0",
    "husky": "0.12.0",
    "jest-cli": "18.1.0",
    "json-loader": "0.5.4",
    "lodash": "4.17.4",
    "postcss": "5.2.8",
    "poststylus": "0.2.3",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-test-renderer": "15.4.2",
    "redux": "3.6.0",
    "redux-batched-actions": "0.1.5",
    "redux-devtools-extension": "1.0.0",
    "redux-thunk": "2.1.0",
    "seamless-immutable": "7.0.1",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "ts-loader": "1.3.1",
    "tslint": "4.3.1",
    "tslint-loader": "3.3.0",
    "tslint-react": "2.2.0",
    "typed-css-modules": "0.1.13",
    "typed-css-modules-loader": "0.0.4",
    "typescript": "2.1.4",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-sources": "0.1.3"
  }
}
