{
  "name": "crossing-tasks",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov",
  "engines": {
    "node": ">=8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/crossing-tasks.git"
  },
  "scripts": {
    "build": "rm -fr build && NODE_ENV=production TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack && npm run build:custom-minify",
    "build:gh": "rm -fr build && NODE_ENV=production ROOT_URL=crossing-tasks TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack",
    "build:custom-minify": "node helpers/minify.js",
    "coverage": "jest --coverage",
    "lint": "tsc --noEmit -p ./ && npm run lint:script && stylint ./src && echo \"There are no error in codestyle.\"",
    "lint:script": "eslint --ext .js,.ts,.tsx ./",
    "build:open": "http-server build --gzip -p 8000 -s -c-1",
    "format:script": "npx prettier --write './**/*.{js,ts,tsx}' && npm run lint:script --fix",
    "start": "TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack-dev-server",
    "test": "jest --no-cache",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:update-snapshots": "jest --no-cache --updateSnapshot",
    "test:backstop": "node ./backstop.js",
    "test:backstop-approve": "node ./backstop.js approve",
    "upgrade": "npm-check --update --exact",
    "update-gh": "./helpers/update-gh.sh",
    "storybook": "TS_NODE_PROJECT=\"tsconfig.webpack.json\" STORYBOOK=1 start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "7.8.3",
    "@babel/plugin-transform-flow-strip-types": "7.9.0",
    "@babel/plugin-transform-react-jsx": "7.9.4",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/cli": "5.3.18",
    "@storybook/react": "5.3.18",
    "@types/autoprefixer": "9.7.2",
    "@types/history": "4.7.8",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/jest": "25.2.3",
    "@types/lodash": "4.14.165",
    "@types/node": "14.0.4",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.56",
    "@types/react-addons-test-utils": "0.14.25",
    "@types/react-dom": "16.9.9",
