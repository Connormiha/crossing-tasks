  "engines": {
    "node": ">=8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/crossing-tasks.git"
  },
  "scripts": {
    "build": "rm -fr build && npm run createTypesCss && NODE_ENV=production webpack",
    "build:gh": "rm -fr build && npm run createTypesCss && NODE_ENV=production ROOT_URL=crossing-tasks webpack",
    "coverage": "jest --coverage --config jest.json",
    "lint": "node lint.js",
    "build:open": "http-server build --gzip -p 8000 -s -c-1",
    "precommit": "NODE_ENV=pre_commit npm run lint",
    "prepush": "npm test",
    "createTypesCss": "node helpers/createStylTs.js",
    "start": "npm run createTypesCss && webpack-dev-server",
    "test": "jest --no-cache --config jest.json",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jest.json",
    "test:update-snapshots": "jest --no-cache --config jest.json --updateSnapshot",
    "test:backstop": "node ./backstop.js",
    "test:backstop-approve": "node ./backstop.js approve",
    "upgrade": "yarn upgrade-interactive --ignore-engines --exact --latest",
    "update-gh": "./helpers/update-gh.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.3",
